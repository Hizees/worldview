<a id="dashboard-link" href="#dashboard"><%= "Welcome, #{current_user.first_name}!" %></a>

<div id="dashboard">
  <div class="modal-box">
    <a id="dashboard-xlink" href="#" title="Close" class="close">X</a>
    <div class="modal-text">

      <h3>Personal Dashboard</h3>
      <img style="width: 200px" src=<%= "#{current_user.image}" %>>

      <h4>Favorited Articles</h4>
      <ul class="dashboard-articles">
      </ul>

      <h4>Favorited Photos</h4>
      <ul class="dashboard-photos">
      </ul>

      <h4>Favorited Tweets</h4>
      <ul class="dashboard-tweets">
      </ul>

    </div>
  </div>
</div>      
